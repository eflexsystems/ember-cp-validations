{
  "name": "@eflexsystems/ember-tracked-validations",
  "version": "7.0.0-beta.0",
  "description": "Ember computed property based validation library",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "engines": {
    "node": "14.* || >= 16"
  },
  "dependencies": {
    "ember-cached-decorator-polyfill": "^0.1.4",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.1.0",
    "ember-validators": "^4.1.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@embroider/test-setup": "^1.8.3",
    "@fortawesome/ember-fontawesome": "^0.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "bootstrap": "^5.2.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.4.2",
    "ember-bootstrap": "^5.1.1",
    "ember-cli": "~4.6.0",
    "ember-cli-app-version": "^5.0.0",
    "ember-cli-autoprefixer": "^2.0.0",
    "ember-cli-code-coverage": "^1.0.3",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-yuidoc": "^0.9.1",
    "ember-code-snippet": "^3.0.0",
    "ember-data": "~4.6.1",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.6.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.10.1",
    "ember-test-selectors": "^6.0.0",
    "ember-truth-helpers": "^3.1.1",
    "ember-try": "^2.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.0.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.1",
    "loader.js": "^4.7.0",
    "moment": "^2.29.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "qunit": "^2.19.1",
    "qunit-dom": "^2.0.0",
    "sass": "^1.54.0",
    "webpack": "^5.74.0",
    "yuidoc-ember-theme": "^2.0.1"
  },
  "keywords": [
    "ember-addon",
    "ember-data",
    "validator",
    "validation",
    "model",
    "input",
    "form"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/eflexsystems/ember-tracked-validations.git"
  },
  "license": "MIT",
  "author": "Jacob Jewell <jacobjewell@eflexsystems.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://offirgolan.github.io/ember-cp-validations"
  },
  "homepage": "https://github.com/offirgolan/ember-cp-validations"
}
